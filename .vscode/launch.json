{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch receptor",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "/var/lib/awx/receptor/receptor",
            "args": [
                "--config",
                "/etc/receptor/receptor.conf"
            ],
            "env": {
                "PATH": "/var/lib/awx/venv/awx/bin:/usr/local/bin:/usr/bin:/usr/local/sbin:/usr/sbin"
            }
        },
        {
            "name": "Launch command-runner",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "/var/lib/awx/receptor/receptor",
            "args": [
                "--log-level", "debug", "--command-runner", "command=sleep", "params=infinity", "unitdir=/tmp/receptor/ip-10-0-7-84.ec2.internal/iY0PgU4i"
            ],
            "env": {
                "PATH": "/var/lib/awx/venv/awx/bin:/usr/local/bin:/usr/bin:/usr/local/sbin:/usr/sbin"
            }
        }
        {
            "name": "Test Current File",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${file}",
            "env": {},
            "args": ["-test.run", "TestWork/kube_worker/results_on_restarted_node"],
            "showLog": true
        }
    ]
}
